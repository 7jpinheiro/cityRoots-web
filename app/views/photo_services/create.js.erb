<% if @photo_service.new_record? %>
alert('Erro a gravar a foto.');
<% else %>
if ($('div.upload') !== undefined) {
    $('div.upload').remove();
}
if ($('div.thumbnails') !== undefined) {
    $('div.thumbnails').append("<%=j render partial: 'photo_services/upload', locals: { photo_service: @photo_service, service: @service } %>");
}
<% end %>
