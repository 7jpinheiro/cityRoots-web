<h1>Listing comment_itineraries</h1>

<table>
  <thead>
    <tr>
      <th>Comment</th>
      <th>Evalutiondate</th>
      <th>Mobile user</th>
      <th>Itinerary</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @comment_itineraries.each do |comment_itinerary| %>
      <tr>
        <td><%= comment_itinerary.comment %></td>
        <td><%= comment_itinerary.evaluationdate %></td>
        <td><%= comment_itinerary.mobile_user_id %></td>
        <td><%= comment_itinerary.itinerary_id %></td>
        <td><%= link_to 'Show', comment_itinerary %></td>
        <td><%= link_to 'Edit', edit_itinerary_comment_itinerary_path(comment_itinerary) %></td>
        <td><%= link_to 'Destroy', comment_itinerary, method: :delete, data: { confirm: 'Tem a certeza?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

