<h1>Listing service_translations</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Schedule</th>
      <th>Language</th>
      <th>Description</th>
      <th>Transport</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @service_translations.each do |service_translation| %>
      <tr>
        <td><%= service_translation.name %></td>
        <td><%= service_translation.schedule %></td>
        <td><%= service_translation.language %></td>
        <td><%= service_translation.description %></td>
        <td><%= service_translation.transport %></td>
        <td><%= link_to 'Show', service_translation %></td>
        <td><%= link_to 'Edit', edit_service_service_translation_path(service_translation) %></td>
        <td><%= link_to 'Destroy', service_translation, method: :delete, data: { confirm: 'Tem a certeza?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

