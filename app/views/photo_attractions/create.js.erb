<% if @photo_attraction.new_record? %>
	alert('Erro a gravar a foto.');
<% else %>
if ($('div.upload') !== undefined) {
    $('div.upload').remove();
}
if ($('div.thumbnails') !== undefined) {
    $('div.thumbnails').append("<%=j render partial: 'photo_attractions/upload', locals: { photo_attraction: @photo_attraction } %>");
}
<% end %>
