<% if @photo_attraction.new_record? %>
alert('Failed');
<% else %>
if ($('div.upload') !== undefined) {
    $('div.upload').remove();
}
if ($('ul.thumbnails') !== undefined) {
    $('ul.thumbnails').append("<%=j render partial: 'photo_attractions/upload', locals: { photo_attraction: @photo_attraction } %>");
}
<% end %>
