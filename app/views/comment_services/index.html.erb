<h1>Listing comment_services</h1>

<table>
  <thead>
    <tr>
      <th>Comment</th>
      <th>Evalutiondate</th>
      <th>Mobile user</th>
      <th>Service</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @comment_services.each do |comment_service| %>
      <tr>
        <td><%= comment_service.comment %></td>
        <td><%= comment_service.evaluationdate %></td>
        <td><%= comment_service.mobile_user_id %></td>
        <td><%= comment_service.service_id %></td>
        <td><%= link_to 'Show', comment_service %></td>
        <td><%= link_to 'Edit', edit_service_comment_service_path(comment_service) %></td>
        <td><%= link_to 'Destroy', comment_service, method: :delete, data: { confirm: 'Tem a certeza?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

