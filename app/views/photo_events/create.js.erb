if @photo_event.new_record? %>
alert('Erro a gravar a foto.');
<% else %>
if ($('div.upload') !== undefined) {
    $('div.upload').remove();
}
if ($('ul.thumbnails') !== undefined) {
    $('ul.thumbnails').append("<%=j render partial: 'photo_events/upload', locals: { photo_event: @photo_event } %>");
}
<% end %>
