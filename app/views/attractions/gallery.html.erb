<%= stylesheet_link_tag "welcome2" %>
<%= csrf_meta_tag %>

<div class="container">

        <div class="row">

            <div class="col-lg-12">
                <h1 class="page-header">Galeria de imagens</h1>
            </div>
            <% @attraction.photo_attractions.each  do |p| %>
            <div class="col-lg-3 col-md-4 col-xs-6 thumb">
                <%= image_tag p.photo.url(:small,false) , { style: "width: 100%;" } %>
                <%= link_to '<i class="fa fa-trash-o"></i>',  p, method: :delete, data: { confirm: 'Tem a certeza?' } %>
                <i class="fa fa-trash-o"> Remover</i>

            </div>
            <% end -%>

        </div>

</br>